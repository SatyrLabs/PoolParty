<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sisyphus.thepoolparty">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ParentLoggedIn" />
        <activity android:name=".ChildLoggedIn" />
        <activity android:name=".NewUser" />
        <activity android:name=".NewUserChild"/>
        <activity android:name=".NewUserParent"/>
        <activity android:name=".NewUserParentCar"/>
        <activity android:name=".NewUserParentPhone"/>

        <activity
            android:name=".dummyActivity"
            android:label="@string/title_activity_dummy"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CreateGroupActivity" />
        <activity
            android:name=".JoinTeamActivity"
            android:label="@string/title_activity_join_team"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>